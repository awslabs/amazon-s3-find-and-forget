{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "title": "Cancel Items Handler",
    "type": "object",
    "properties": {
        "body": {
            "type": "object",
            "description": "Body parameters for the request",
            "properties": {
                "Matches": {
                    "description": "The list of Match IDs to remove from the deletion queue",
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "MatchId": {
                                "type": "string",
                                "description": "The Match ID to remove from the deletion queue"
                            },
                            "CreatedAt": {
                                "type": "integer",
                                "description": "The Creation date of the Match ID"
                            }
                        },
                        "required": ["MatchId", "CreatedAt"]
                    }
                }
            },
            "required": ["Matches"]
        }
    },
    "required": ["body"]
}

