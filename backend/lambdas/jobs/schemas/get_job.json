{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "title": "Get Job Handler",
    "type": "object",
    "properties": {
        "pathParameters": {
            "description": "Path parameters for the request",
            "type": "object",
            "job_id": {
                "description": "ID of the Job",
                "type": "string",
                "pattern": "^([A-Za-z0-9-])+$"
            },
            "required": ["job_id"]
        }
    },
    "required": ["pathParameters"]
}

