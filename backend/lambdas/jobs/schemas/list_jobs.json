{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "List Jobs Handler",
    "type": "object",
    "properties": {
        "queryStringParameters": {
            "description": "Query string parameters for the request",
            "type": "object",
            "properties": {
                "start_at": {
                    "description": "Starting watermark",
                    "type": "string",
                    "pattern": "^[0-9]+$"
                },
                "page_size": {
                    "description": "Maximum page size",
                    "type": "string",
                    "pattern": "^[0-9]+$"
                }
            }
        }
    }
}

