{
  "name": "amazon-s3-find-and-forget-frontend",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/awslabs/amazon-s3-find-and-forget"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "aws-amplify": "^2.2.0",
    "aws-amplify-react": "^3.1.1",
    "react": "^16.12.0",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-dom": "^16.12.0",
    "react-json-view": "^1.19.1",
    "react-scripts": "^3.3.1"
  },
  "scripts": {
    "build": "REACT_APP_REPO_URL=$npm_package_repository_url react-scripts build",
    "eject": "react-scripts eject",
    "package": "cd build && zip -r ../../frontend.zip . -x *settings.js",
    "start": "export HTTPS=true && REACT_APP_REPO_URL=$npm_package_repository_url react-scripts start",
    "test": "export CI=true && react-scripts test"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
